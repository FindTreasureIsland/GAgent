{
  "name": "@google/gemini-web-server",
  "version": "0.1.0",
  "description": "Gemini Web Server",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:telemetry": "GEMINI_TELEMETRY_ENABLED=1 GEMINI_TELEMETRY_TARGET=local GEMINI_TELEMETRY_LOG_PROMPTS=true GEMINI_TELEMETRY_OUTFILE=telemetry.jsonl node dist/index.js",
    "dev": "tsc --watch & nodemon dist/index.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write ."
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "@google/gemini-cli-core": "file:../core",
    "@google/gemini-cli": "file:../cli"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.11.24",
    "typescript": "^5.3.3",
    "nodemon": "^3.0.1"
  }
}
